<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <servlet>
    <servlet-name>MainController</servlet-name>
    <servlet-class>com.iqmsoft.jcache.jcacheinjection.MainController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ControllerDelete</servlet-name>
    <servlet-class>com.iqmsoft.jcache.jcacheinjection.ControllerDelete</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ControllerGet</servlet-name>
    <servlet-class>com.iqmsoft.jcache.jcacheinjection.ControllerGet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MainController</servlet-name>
    <url-pattern>/MainController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ControllerDelete</servlet-name>
    <url-pattern>/ControllerDelete</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ControllerGet</servlet-name>
    <url-pattern>/ControllerGet</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>30</session-timeout>
  </session-config>
</web-app>